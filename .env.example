# ALPACA_API_KEY_REF=openclaw:env:default:ALPACA_API_KEY
# ALPACA_SECRET_KEY_REF=openclaw:file:filemain:/providers/alpaca/secretKey
# BRAVE_API_KEY_REF=openclaw:exec:vault:providers/brave/apiKey

